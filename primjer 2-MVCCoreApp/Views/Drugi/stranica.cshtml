@{
    // tu ide blok CSH koda
    // mogu i @i

    List<string> imena = new List<string>()
    {
        "Pajo Patak",
        "Šiljo i Miki",
        "Zekoslav Mrkva"
    };

}

<h2> naša stranica </h2>
<!-- 
    Layout (mastrepage) je definiran u _Layout.cshtml
    -->

<ol>
@{ 
    foreach (string ime in imena)
    {
        <li>@ime</li>
    }
}
</ol>